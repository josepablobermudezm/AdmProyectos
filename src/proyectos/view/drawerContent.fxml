<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proyectos.controller.DrawerContentController">
   <children>
      <AnchorPane prefHeight="800.0" prefWidth="200.0" stylesheets="@newCascadeStyleSheet.css">
         <children>
            <VBox prefHeight="800.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <ImageView fx:id="image" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  <JFXButton fx:id="btnAdministradores" onAction="#Administradores" prefHeight="150.0" prefWidth="200.0" style="-fx-cursor: Hand;" text="Administradores" />
                  <JFXButton fx:id="btnProyectos" onAction="#btnProyectos" prefHeight="150.0" prefWidth="200.0" style="-fx-cursor: Hand;" text="Proyectos">
                  </JFXButton>
                  <JFXButton fx:id="btnResumen" onAction="#btnResumen" prefHeight="150.0" prefWidth="200.0" style="-fx-cursor: Hand;" text="Resumen de Seguimientos">
                  </JFXButton>
                  <JFXButton fx:id="btnActividades" onAction="#btnActividades" prefHeight="150.0" prefWidth="200.0" style="-fx-cursor: Hand;" text="Actividades Proyectos">
                  </JFXButton>
                  <JFXButton fx:id="btnExit" onAction="#exit" prefHeight="150.0" prefWidth="200.0" style="-fx-cursor: Hand;" text="Exit">
                  </JFXButton>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</VBox>
